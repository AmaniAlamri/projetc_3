<%= form_for @course do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

<%= f.label :field %>
<%# <%= f.collection_select :actor_id, Actor.all, :id, :name %> %>
  <%= f.text_field :field %>

